public class EulerSix
{
  public static void main ( String[] args )
  {
    int sum = 0;
	  int sumSqrd;
	  int srqdSum=0;
	  int diff;
    for(int i=0; i<101; i++){
		sum+=i;
	}
	sumSqrd = sum * sum;

	for (int j =0; j <101; j++){
		srqdSum = srqdSum + j*j;
	}

	diff = sumSqrd- srqdSum;
	System.out.println(diff);
  }

}
